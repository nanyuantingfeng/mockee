#!/usr/bin/env node

var program = require('commander')

program
  .version('1.0.0')
  .option('-p, --port <n>', '端口号')
  .option('-m, --mock <s>', '跟路径')
  .option('-b, --base <s>', 'URL修正')
  .parse(process.argv)

var MockServer = require('../index')

var {mock, port, base} = program

MockServer({mock, port, base})
